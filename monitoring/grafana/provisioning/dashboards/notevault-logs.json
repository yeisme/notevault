{
    "id": null,
    "title": "NoteVault Logs",
    "tags": [
        "notevault",
        "logs"
    ],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 0,
    "refresh": "5s",
    "panels": [
        {
            "id": 1,
            "title": "Container Logs",
            "type": "logs",
            "gridPos": {
                "h": 16,
                "w": 24,
                "x": 0,
                "y": 0
            },
            "options": {
                "dedupStrategy": "none",
                "enableLogDetails": true,
                "showTime": true,
                "sortOrder": "Descending",
                "wrapLogMessage": true
            },
            "targets": [
                {
                    "expr": "{container=~\"notevault-.*\"}",
                    "refId": "A"
                }
            ]
        }
    ],
    "time": {
        "from": "now-1h",
        "to": "now"
    }
}